<GridPane alignment="CENTER" prefHeight="500" prefWidth="700" styleClass="root" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.kyllian.netflixstatistix.controllers.WatchedDataOverviewController">

    <padding>
        <Insets bottom="10" left="25" right="25" top="25" />
    </padding>

    <ImageView fx:id="logo" fitHeight="100" fitWidth="100" preserveRatio="true" translateX="-110" translateY="-110" GridPane.halignment="LEFT" GridPane.valignment="TOP" />


    <TableView fx:id="table" prefHeight="341.0" prefWidth="421.0" GridPane.columnSpan="4" GridPane.halignment="CENTER" GridPane.rowIndex="2">
        <columns>
            <TableColumn fx:id="tableTitle" prefWidth="148.80001145601273" text="programs watched">
            </TableColumn>
            <TableColumn fx:id="tableID" prefWidth="271.19996643066406" text="programs for adding or removing">
            </TableColumn>
        </columns>
    </TableView>
   <Button mnemonicParsing="false" text="Add program" GridPane.rowIndex="3" />
   <Button mnemonicParsing="false" text="Remove program" GridPane.columnIndex="3" GridPane.rowIndex="3" />
   <Label text="ProfileName" />
   <Label alignment="TOP_LEFT" prefHeight="18.0" prefWidth="45.0" text="Age" GridPane.rowIndex="1" />
   <Button mnemonicParsing="false" onMouseClicked="#back" text="Return" GridPane.columnIndex="4" GridPane.rowIndex="3" />
